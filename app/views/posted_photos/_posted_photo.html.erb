<div>
  <p><%= posted_photo.title %></p>
    <% if session[:access_token].present? %>
    <%= button_to 'ツイートする', oauth_post_tweet_path(posted_photo: { title: posted_photo.title, image: posted_photo.image }), method: :post %>
  <% end %>
  <%= image_tag "/posted_photos/#{posted_photo.image}" %>
</div>